/*************************************************************************/
/*	Linker Command File ----- simple.CMD  for TMS320C32		    */
/*************************************************************************/

	/*****************************************************************/
	/*	 Specify the Input Files				 */
	/*****************************************************************/
		start.obj
		NorFlash.obj
		z85c30.obj
		Disk.obj
		GpProface.obj
		DataPro.obj
		LED_DOWNLOAD.obj
		Main.obj

	/*****************************************************************/
	/*	 Specify Linker Options 				 */
	/*****************************************************************/
		-o main.out
		-m main.map
		-c
		-stack 0x2000
		-l rts30.lib
		/*-w*/
		/*-x*/

	/*****************************************************************/
	/*	 Specify the Memory Configuration			 */
	/*****************************************************************/

	MEMORY
	{
		FROM  : origin =  0900000h	length = 80000h
		IRAM  : origin =  087FE10h	length = 0001F0h
		SRAM  : origin =  0A00000h	length = 10000h
	}

	/*****************************************************************/
	/*	 Specify the Output Sections				 */
	/*****************************************************************/

	SECTIONS {
		/*.text  : { start.obj(.text) }	> SRAM*/
		/*.text  : { Main.obj(.text) }  > SRAM*/
		
		.text : > SRAM

		.cinit : { }	> SRAM
		.const : { }	> SRAM
		.data  : { }	> SRAM
		.stack : { }	> SRAM
		.bss   : { }	> SRAM
		/*.sysmem: { }  > SRAM*/
	}

/*************************************************************************/
/*   End of Command File						 */
/*************************************************************************/
